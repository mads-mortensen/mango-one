{
  "name": "Mango-one",
  "version": "1.0.1",
  "description": "Simple template: Hapi, Webpack (+ Sass, Babel), Vue, Bulma",
  "scripts": {
    "start": "npm run build && concurrently --kill-others \"npm run webpack-watch\" \"npm run server\"",
    "clean": "rimraf dist",
    "build": "npm run clean && npm run webpack",
    "webpack": "webpack",
    "webpack-watch": "webpack --watch",
    "webpack-test": "webpack --config ./config/test/webpack.test.config.js",
    "webpack-test-watch": "webpack --config ./config/test/webpack.test.config.js --watch",
    "server": "node server/server.js",
    "clean-test": "rimraf test",
    "build-test": "npm run clean-test && npm run webpack-test",
    "karma": "karma start config/test/karma.conf.js",
    "build-production": "npm run clean && webpack -p"
  },
  "author": "Mads Mortensen",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-preset-es2015": "^6.22.0",
    "concurrently": "^3.4.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.16.1",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-jasmine": "^2.2.0",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "file-loader": "^0.10.0",
    "html-webpack-plugin": "^2.28.0",
    "ignore-loader": "^0.1.2",
    "jasmine": "^2.5.3",
    "jasmine-core": "^2.5.2",
    "karma": "^1.6.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.1.0",
    "karma-webpack": "^2.0.3",
    "node-sass": "^4.5.0",
    "sass-loader": "^6.0.2",
    "style-loader": "^0.13.1",
    "vue-loader": "^11.3.4",
    "vue-template-compiler": "2.3.3",
    "webpack": "^2.2.1"
  },
  "dependencies": {
    "bulma": "^0.3.1",
    "hapi": "^16.1.0",
    "inert": "^4.1.0",
    "vue": "2.3.3",
    "vue-router": "^2.5.3"
  }
}
